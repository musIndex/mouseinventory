    <url-pattern>/SubmitChangeRequest</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>edu.ucsf.mousedatabase.listeners.ContextListener</listener-class>
  </listener>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_MGI_DRIVER_NAME</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>org.postgresql.Driver</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_MGI_DATABASE_URL</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>jdbc:postgresql://mgi-adhoc.jax.org:5432/mgd</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_MGI_DATABASE_USER</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>defaultUserName</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_MGI_DATABASE_PW</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>defaultPassword</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_SMTP_SERVER</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_SMTP_USER</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_SMTP_PW</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_SMTP_PORT</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
  </env-entry>
  <env-entry>
    <env-entry-name>MOUSEDATABASE_SMTP_SSL_ENABLED</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
  </env-entry>
  <env-entry>
    <env-entry-name>GOOGLE_ANALYTICS_ACCOUNT</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value></env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>GOOGLE_ANALYTICS_DOMAIN_SUFFIX</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value></env-entry-value>
  </env-entry>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>Admin Pages</web-resource-name>
      <url-pattern>/admin/*</url-pattern>
      <http-method>GET</http-method>
      <http-method>POST</http-method>
    </web-resource-collection>
    <auth-constraint>
      <role-name>administrator</role-name>
    </auth-constraint>
  </security-constraint>
  <login-config>
    <auth-method>FORM</auth-method>
    <form-login-config>
      <form-login-page>/adminLoginForm.jsp</form-login-page>
      <form-error-page>/accessDenied.jsp</form-error-page>
    </form-login-config>
  </login-config>
  <security-role>
    <description>Mouse Inventory Site Administrator</description>
    <role-name>administrator</role-name>
  </security-role>
  <error-page>
    <error-code>500</error-code>
    <location>/error.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>
  <resource-ref>
    <description>Connection Pool</description>
    <res-ref-name>jdbc/mouse_inventory</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <description></description>
    <display-name>CsvServletNew</display-name>
    <servlet-name>CsvServletNew</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.CsvServletNew</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CsvServletNew</servlet-name>
    <url-pattern>/MouseList2</url-pattern>
  </servlet-mapping>
</web-app>
