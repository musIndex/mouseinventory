<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>mouseinventory</display-name>
  <welcome-file-list>
    <welcome-file>welcome.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ValidationServlet</display-name>
    <servlet-name>ValidationServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.ValidationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidationServlet</servlet-name>
    <url-pattern>/validate</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ReportServlet</display-name>
    <servlet-name>ReportServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.ReportServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>jersey-serlvet</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
        <param-name>jersey.config.server.provider.packages</param-name>
        <param-value>edu.ucsf.mousedatabase.admin</param-value>
    </init-param>
    <init-param>
        <param-name>jersey.config.server.provider.classnames</param-name>
        <param-value>org.glassfish.jersey.media.multipart.MultiPartFeature</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
</servlet>
  <servlet-mapping>
    <servlet-name>ReportServlet</servlet-name>
    <url-pattern>/report</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ImportTransferServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.ImportServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImportTransferServlet</servlet-name>
    <url-pattern>/admin/Import</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MgiDataServlet</display-name>
    <servlet-name>MgiDataServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.MgiDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MgiDataServlet</servlet-name>
    <url-pattern>/mgidata</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PdfServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.PdfServlet</servlet-class>
  </servlet>
  <servlet>
    <description>
  	</description>
    <display-name>SendMailServlet</display-name>
    <servlet-name>SendMailServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.SendMailServlet</servlet-class>
  </servlet>
  <servlet>
    <description>
  	</description>
    <display-name>UpdateTemplateServlet</display-name>
    <servlet-name>UpdateTemplateServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.UpdateTemplateServlet</servlet-class>
  </servlet>
  <servlet>
    <description>
  	</description>
    <display-name>UpdateSettingServlet</display-name>
    <servlet-name>UpdateSettingServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.UpdateSettingServlet</servlet-class>
  </servlet>
  <servlet>
    <description>
  	</description>
    <display-name>UpdateFacilityOrderServlet</display-name>
    <servlet-name>UpdateFacilityOrderServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.UpdateFacilityOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <description>
  	</description>
    <display-name>UpdateSettingOrderServlet</display-name>
    <servlet-name>UpdateSettingOrderServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.UpdateSettingOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <description>
  	</description>
    <display-name>SubmitChangeRequestServlet</display-name>
    <servlet-name>SubmitChangeRequestServlet</servlet-name>
    <servlet-class>edu.ucsf.mousedatabase.servlets.SubmitChangeRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PdfServlet</servlet-name>
    <url-pattern>/MouseList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SendMailServlet</servlet-name>
    <url-pattern>/admin/SendMail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateTemplateServlet</servlet-name>
    <url-pattern>/admin/UpdateTemplate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateSettingServlet</servlet-name>
    <url-pattern>/admin/UpdateSetting</url-pattern>
